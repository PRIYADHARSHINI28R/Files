					},
					"AzureSqlDatabase2_connectionString": {
						"value": "[parameters('AzureSqlDatabase2_connectionString')]"
					},
					"AzureSqlDatabasead_connectionString": {
						"value": "[parameters('AzureSqlDatabasead_connectionString')]"
					}
				}
			},
